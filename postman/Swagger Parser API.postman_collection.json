				{
					"name": "导入Swagger文档（YAML内容）",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"content\": \"openapi: 3.0.0\\ninfo:\\n  title: 测试API\\n  version: 1.0.0\\nservers:\\n  - url: http://localhost:8080\\npaths:\\n  /api/users:\\n    get:\\n      summary: 获取用户列表\\n      operationId: getUsers\\n      parameters:\\n        - name: page\\n          in: query\\n          schema:\\n            type: integer\\n            example: 1\\n      responses:\\n        '200':\\n          description: 成功\\n          content:\\n            application/json:\\n              schema:\\n                type: object\\n                properties:\\n                  data:\\n                    type: array\\n                    items:\\n                      type: object\\n                      properties:\\n                        id:\\n                          type: integer\\n                        name:\\n                          type: string\",\n  \"contentType\": \"yaml\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseUrl}}/swagger/import",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"swagger",
								"import"
							]
						},
						"description": "通过YAML内容导入Swagger文档"
					},
					"response": []
				},
				{
					"name": "导入Swagger文档（文件上传）",
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "file",
									"type": "file",
									"src": [],
									"description": "上传Swagger文档文件，支持.json、.yaml、.yml格式"
								}
							]
						},
						"url": {
							"raw": "{{baseUrl}}/swagger/import/file",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
