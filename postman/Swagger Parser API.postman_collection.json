	"item": [
		{
			"name": "导入Swagger文档",
			"item": [
				{
					"name": "导入Swagger文档（JSON内容）",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"content\": \"{\\\"openapi\\\":\\\"3.0.0\\\",\\\"info\\\":{\\\"title\\\":\\\"测试API\\\",\\\"version\\\":\\\"1.0.0\\\"},\\\"servers\\\":[{\\\"url\\\":\\\"http://localhost:8080\\\"}],\\\"paths\\\":{\\\"/api/users\\\":{\\\"get\\\":{\\\"summary\\\":\\\"获取用户列表\\\",\\\"operationId\\\":\\\"getUsers\\\",\\\"parameters\\\":[{\\\"name\\\":\\\"page\\\",\\\"in\\\":\\\"query\\\",\\\"schema\\\":{\\\"type\\\":\\\"integer\\\",\\\"example\\\":1}}],\\\"responses\\\":{\\\"200\\\":{\\\"description\\\":\\\"成功\\\",\\\"content\\\":{\\\"application/json\\\":{\\\"schema\\\":{\\\"type\\\":\\\"object\\\",\\\"properties\\\":{\\\"data\\\":{\\\"type\\\":\\\"array\\\",\\\"items\\\":{\\\"type\\\":\\\"object\\\",\\\"properties\\\":{\\\"id\\\":{\\\"type\\\":\\\"integer\\\"},\\\"name\\\":{\\\"type\\\":\\\"string\\\"}}}}}}}}}}}}}\",\n  \"contentType\": \"json\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{baseUrl}}/swagger/import",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"swagger",
								"import"
							]
						},
						"description": "通过JSON内容导入Swagger文档"
					},
					"response": []
				},
